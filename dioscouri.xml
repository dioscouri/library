<?xml version="1.0" encoding="utf-8"?>
<extension version="1.6" type="library" method="upgrade">
	<name>Dioscouri Library</name>
	<libraryname>dioscouri</libraryname>
	<author>Dioscouri Design</author>
	<creationDate>December 2011</creationDate>
	<copyright>Copyright (C) 2011 Dioscouri Design. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later</license>
	<authorEmail>info@dioscouri.com</authorEmail>
	<authorUrl>www.dioscouri.com</authorUrl>
	<version>1.0.0</version>
	<description></description>
    
	<files>
		<file>dioscouri.php</file>
        <file>loader.php</file>
        <folder>component</folder>
        <folder>highroller</folder>
        <folder>library</folder>
        <folder>tools</folder>
	</files>
    
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.lib_dioscouri.ini</language>
	</languages>
    
    <!-- Media -->
    <media destination="dioscouri" folder="media">
        <folder>css</folder>
        <folder>images</folder>
        <folder>js</folder>
        <file>index.html</file>
    </media>
    
</extension>
